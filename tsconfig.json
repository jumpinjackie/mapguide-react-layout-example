{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es5",
        "importHelpers": true,
        "strictNullChecks": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "noImplicitAny": true,
        "removeComments": false,
        "sourceMap": true,
        "jsx": "react"
    },
    "exclude": [
        "node_modules"
    ]
}